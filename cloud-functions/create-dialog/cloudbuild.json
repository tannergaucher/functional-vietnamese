{
  "steps": [
    {
      "name": "gcr.io/google.com/cloudsdktool/cloud-sdk",
      "args": [
        "gcloud",
        "functions",
        "deploy",
        "createDialog",
        "--gen2",
        "--region=us-east1",
        "--source=./index.js",
        "--runtime=nodejs18",
        "--entry-point=createDialog",
        "--trigger-topic=create-dialog"
      ],
      "secretEnv": ["DATABASE_URL", "OPENAI_API_KEY", "OPENAI_MODEL"]
    }
  ],
  "availableSecrets": {
    "secretManager": [
      {
        "versionName": "projects/625494448696/secrets/DATABASE_URL/versions/1",
        "env": "DATABASE_URL"
      },
      {
        "versionName": "projects/625494448696/secrets/OPENAI_API_KEY/versions/1",
        "env": "OPENAI_API_KEY"
      },
      {
        "versionName": "projects/625494448696/secrets/OPENAI_MODEL/versions/1",
        "env": "OPENAI_MODEL"
      }
    ]
  },
  "options": {
    "logging": "CLOUD_LOGGING_ONLY"
  }
}
